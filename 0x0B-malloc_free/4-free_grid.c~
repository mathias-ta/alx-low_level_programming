#include "holberton.h"
#include <stdio.h>
#include <stdlib.h>

/**
 *free_grid - frees a grid
 *@grid: grid to be freed
 *@height: height of the grid
 *Return: void
 */

void free_grid(int **grid, int height)
{
int i = 0, j = 0;
if (grid != NULL)
{

}
while (i < height)
{
p[i] = malloc(sizeof(int) * width);
while (j < width)
{
if (p[i] == NULL)
{
for (i = 0; i < height; i++)
{
free(p[i]);
}
free(p);
return (NULL);
}
p[i][j] = 0;
j++;
}
j = 0;
i++;
}

return (p);
}
